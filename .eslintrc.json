{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
      "emcaVersion": 7,
      "emcafeatures": {
        "jsx": true
      }
    },
    "rules": {
      "strict": 0,
      "react/no-multi-comp": 0,
      "react/prefer-stateless-function": 0
    },
    "plugins": [
        "react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
}